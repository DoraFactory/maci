{
  "name": "@dorafactory/maci-e2e",
  "version": "1.0.0",
  "description": "End-to-end tests for MACI contracts using cosmwasm-simulate",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "pretest": "ts-node scripts/checkCircuits.ts",
    "test": "pnpm run mocha-test tests/**/*.test.ts",
    "test:amaci": "pnpm run mocha-test tests/amaci.e2e.test.ts",
    "test:maci": "pnpm run mocha-test tests/maci.e2e.test.ts",
    "test:registry": "pnpm run mocha-test tests/registry.e2e.test.ts",
    "test:advanced": "pnpm run mocha-test tests/advanced.e2e.test.ts",
    "test:stateTree": "pnpm run mocha-test tests/state-tree.e2e.test.ts",
    "test:batch-publish": "pnpm run mocha-test tests/batch-publish.e2e.test.ts",
    "test:key-rotation": "pnpm run mocha-test tests/key-rotation.e2e.test.ts",
    "test:add-new-key": "pnpm run mocha-test tests/add-new-key.e2e.test.ts",
    "generate:vectors": "ts-node poseidon-test/generate-vectors.ts",
    "pretest:poseidon": "pnpm run generate:vectors",
    "test:poseidon": "pnpm run mocha-test tests/poseidon-consistency.e2e.test.ts",
    "generate:crypto-vectors": "ts-node crypto-test/generate-vectors.ts",
    "pretest:crypto": "pnpm run generate:crypto-vectors",
    "test:crypto": "pnpm run mocha-test tests/crypto-consistency.e2e.test.ts",
    "test:amaci-crypto-core": "pnpm run mocha-test tests/amaci-crypto-core.e2e.test.ts",
    "generate:baby-jubjub-vectors": "cd .. && cargo run --bin generate-baby-jubjub-vectors",
    "generate:eddsa-poseidon-vectors": "cd .. && cargo run --bin generate-eddsa-poseidon-vectors",
    "generate:zk-kit-vectors": "pnpm run generate:baby-jubjub-vectors && pnpm run generate:eddsa-poseidon-vectors",
    "test:baby-jubjub": "pnpm run mocha-test tests/baby-jubjub.e2e.test.ts",
    "test:eddsa-poseidon": "pnpm run mocha-test tests/eddsa-poseidon.e2e.test.ts",
    "test:zk-kit": "pnpm run test:baby-jubjub && pnpm run test:eddsa-poseidon",
    "mocha-test": "NODE_OPTIONS=--max-old-space-size=4096 ts-mocha --project tsconfig.test.json --exit --timeout 900000",
    "download-zkeys": "ts-node scripts/downloadZkeys.ts",
    "extract-vkeys": "ts-node scripts/extractVkeys.ts",
    "setup-circuits": "pnpm download-zkeys && pnpm extract-vkeys",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "keywords": [
    "maci",
    "e2e",
    "cosmwasm",
    "testing"
  ],
  "author": "team@dorafactory.org",
  "license": "ISC",
  "dependencies": {
    "@oraichain/cw-simulate": "^2.8.23",
    "@zk-kit/baby-jubjub": "^1.0.3",
    "@zk-kit/eddsa-poseidon": "^1.1.0",
    "@zk-kit/poseidon-cipher": "^0.3.2",
    "@zk-kit/utils": "^1.4.1",
    "@dorafactory/maci-sdk": "workspace:*",
    "@dorafactory/maci-circuits": "workspace:*",
    "@cosmjs/cosmwasm-stargate": "^0.37.0",
    "@cosmjs/amino": "^0.37.0",
    "@cosmjs/proto-signing": "^0.37.0",
    "@cosmjs/stargate": "^0.37.0",
    "circomkit": "^0.3.4",
    "snarkjs": "^0.7.4",
    "ffjavascript": "^0.2.46"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.19",
    "@types/secp256k1": "^4.0.6",
    "@types/tar": "^6.1.13",
    "chai": "^4.3.10",
    "mocha": "^11.7.2",
    "secp256k1": "^5.0.0",
    "tar": "^7.4.3",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  }
}
